ieID } = useParams();\n  const { path } = useRouteMatch();\n\n  useEffect(() => {\n    let id = movieID && movieID.match(/[a-z0-9]+$/)[0]; // вытягуем с строки url сгенереной slug наш ид фильма\n    console.log(movieID);\n    console.log(id);\n    getMovieDetails(id).then(data => setMovie(data));\n  }, []);\n\n  return (\n    <>\n      <Section>{movie && <OneMovieDetails movie={movie} />}</Section>\n      <Section>\n        {movie && (\n          <Route exact path={`${path}/cast`}>\n            <CastBlock />\n          </Route>\n        )}\n        {movie && (\n          <Route exact path={`${path}/reviews`}>\n            <ReviewsBlocks />\n          </Route>\n        )}\n      </Section>\n    </>\n  );\n}\n",["149","150"],"D:\\fs-online\\REACT\\mod-5\\goit-react-hw-05-hooks-movies\\src\\components\\MovieSearch\\MovieSearch.jsx",["151","152","153","154"],"D:\\fs-online\\REACT\\mod-5\\goit-react-hw-05-hooks-movies\\src\\views\\HomePage\\HomePage.js",["155","156","157"],"D:\\fs-online\\REACT\\mod-5\\goit-react-hw-05-hooks-movies\\src\\context\\FilmsReturnProvider.js",[],"D:\\fs-online\\REACT\\mod-5\\goit-react-hw-05-hooks-movies\\src\\views\\NotFound\\NotFound.js",[],"D:\\fs-online\\REACT\\mod-5\\goit-react-hw-05-hooks-movies\\src\\components\\AnimationBackground\\AnimationBackground.js",["158"],"import React, { useRef, useState } from 'react';\n//R3F\nimport { Canvas, useFrame } from 'react-three-fiber';\n// Deai - R3F\nimport { softShadows, MeshWobbleMaterial } from 'drei';\n// React Spring\nimport { useSpring, a } from '@react-spring/three';\nimport { CanvasAnimBlock } from './StyledComponent';\n\n// soft Shadows\nsoftShadows();\n\nconst SpinningMesh = ({ position, color, speed, args }) => {\n  //ref to target the mesh\n  const mesh = useRef();\n  //useFrame allows us to re-render/update rotation on each frame\n  useFrame(() => (mesh.current.rotation.x = mesh.current.rotation.y += 0.01));\n  //Basic expand state\n  const [hovered, setHover] = useState(false);\n\n  return (\n    <a.mesh\n      position={position}\n      ref={mesh}\n      onPointerOver={event => setHover(true)}\n      onPointerOut={event => setHover(false)}\n      castShadow\n    >\n      <boxBufferGeometry attach=\"geometry\" args={args} />\n      <MeshWobbleMaterial\n        color={hovered ? '#FF9500' : color}\n        speed={speed}\n        attach=\"material\"\n        factor={0.6}\n      />\n    </a.mesh>\n  );\n};\n\nconst AnimationBackground = () => {\n  return (\n    <>\n      {/* Our Scene & Camera is already built into our canvas */}\n      <CanvasAnimBlock>\n        <Canvas\n          className=\"CanvasStyle\"\n          colorManagement\n          shadowMap\n          camera={{ position: [-5, 2, 10], fov: 80 }}\n        >\n          {/* This light makes things look pretty */}\n          <ambientLight intensity={0.3} />\n          {/* Our main source of light, also casting our shadow */}\n          <directionalLight\n            castShadow\n            position={[0, 10, 0]}\n            intensity={1.5}\n            shadow-mapSize-width={1024}\n            shadow-mapSize-height={1024}\n            shadow-camera-far={50}\n            shadow-camera-left={-10}\n            shadow-camera-right={10}\n            shadow-camera-top={10}\n            shadow-camera-bottom={-10}\n          />\n          {/* A light to help illumnate the spinning boxes */}\n          <pointLight position={[-10, 0, -20]} intensity={0.5} />\n          <pointLight position={[0, -10, 0]} intensity={1.5} />\n          <group>\n            {/* This mesh is the plane (The floor) */}\n            <mesh\n              rotation={[-Math.PI / 2, 0, 0]}\n              position={[0, -3, 0]}\n              receiveShadow\n            >\n              <planeBufferGeometry attach=\"geometry\" args={[100, 100]} />\n              <shadowMaterial attach=\"material\" opacity={0.3} />\n            </mesh>\n            <SpinningMesh\n              position={[0, 1, 0]}\n              color=\"#8A94A9\"\n              args={[3, 2, 1]}\n              speed={2}\n            />\n            <SpinningMesh position={[-2, 1, -5]} color=\"#BEB1A0\" speed={6} />\n            <SpinningMesh position={[5, 1, -2]} color=\"#BEB1A0\" speed={6} />\n          </group>\n          {/* Allows us to move the canvas around for different prespectives */}\n        </Canvas>\n      </CanvasAnimBlock>\n    </>\n  );\n};\n\nexport default AnimationBackground;\n","D:\\fs-online\\REACT\\mod-5\\goit-react-hw-05-hooks-movies\\src\\components\\Spiner\\Spiner.jsx",[],"D:\\fs-online\\REACT\\mod-5\\goit-react-hw-05-hooks-movies\\src\\components\\PageHeader\\PageHeader.jsx",[],"D:\\fs-online\\REACT\\mod-5\\goit-react-hw-05-hooks-movies\\src\\components\\PageMain\\PageMain.jsx",[],"D:\\fs-online\\REACT\\mod-5\\goit-react-hw-05-hooks-movies\\src\\components\\PageNav\\PageNav.jsx",[],"D:\\fs-online\\REACT\\mod-5\\goit-react-hw-05-hooks-movies\\src\\context\\context.js",[],"D:\\fs-online\\REACT\\mod-5\\goit-react-hw-05-hooks-movies\\src\\views\\HomePage\\StyledComponent.js",[],"D:\\fs-online\\REACT\\mod-5\\goit-react-hw-05-hooks-movies\\src\\service\\fetchApi.js",[],"D:\\fs-online\\REACT\\mod-5\\goit-react-hw-05-hooks-movies\\src\\components\\AnimationBackground\\StyledComponent.js",[],"D:\\fs-online\\REACT\\mod-5\\goit-react-hw-05-hooks-movies\\src\\components\\OneMovieDetails\\OneMovieDetails.jsx",["159"],"import { useEffect, useContext } from 'react';\nimport { useLocation, useRouteMatch } from 'react-router-dom';\nimport {\n  ButtonBack,\n  MovieDescription,\n  MovieGenresItem,\n  MovieGenresList,\n  DateRelease,\n  Title,\n  ContentBox,\n  PosterImg,\n  Container,\n  NavList,\n  NavItem,\n  NavContainer,\n  TitleNav,\n} from './styledComponent';\nimport createImgUrl from '../../service/createImgUrl';\nimport { NavLink } from 'react-router-dom';\nimport DefaultImage from '../../img/default-image.jpg';\nimport FilmReturnContext from '../../context/context';\n\nexport default function OneMovieDetail({\n 